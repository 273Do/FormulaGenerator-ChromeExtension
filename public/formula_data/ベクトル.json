  },
  "転置": {
    "転置1": " {}^t \\boldsymbol{A} ",
    "転置2": " \\boldsymbol{A}^{ \\mathrm{T} } ",
    "n次元列ベクトルの転置": " \\boldsymbol{a} = ( a_1, a_2, \\cdots , a_n )^{ \\mathrm{T} } = \\left( \\begin{array}{c} a_1 \\\\ a_2 \\\\ \\vdots \\\\ a_n \\end{array}\\right) "
  }
}
